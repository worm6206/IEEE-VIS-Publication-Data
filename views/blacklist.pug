  link(href='https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.4/semantic.min.css', rel='stylesheet')
  style.
    .container{
      padding:20px 0 20px 0;
    }
  body
    div.ui.container 
      h2 Click on keyword to remove from blacklist
      a(href=".") back
